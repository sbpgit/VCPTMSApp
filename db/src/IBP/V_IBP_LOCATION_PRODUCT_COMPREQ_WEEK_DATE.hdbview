VIEW V_IBP_LOCATION_PRODUCT_COMPONENT_WEEK_DATE AS
 SELECT "CAL_DATE" AS "WEEK_DATE", "LOCATION_ID" AS "LOCID", "PRODUCT_ID" AS "PRDID", "COMPONENT" AS"PRDFR", 
 "STRUC_NODE" AS "VCSTRUCTURENODE", "MODEL_VERSION",
 "VERSION" AS "VERSION", "SCENARIO" AS  "SCENARIO", 
 "COMP_QTY" AS "COMPONENTREQUIREMENTQTY" FROM V_COMP_REQ ;
