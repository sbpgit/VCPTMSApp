VIEW V_IBP_SALESH_WEEK_DATE AS
SELECT SALES_DOC, ADD_DAYS (TO_DATE ("MAT_AVAILDATE", 'YYYY-MM-DD'), 6-WEEKDAY (TO_DATE ("MAT_AVAILDATE", 'YYYY-MM-DD'))) AS "WEEK_DATE",
LOCATION_ID, PRODUCT_ID, ORD_QTY, CUSTOMER_GROUP, "VCCLASS", CHAR_NUM, CHARVAL_NUM 
FROM V_IBP_SALESH
ORDER BY "MAT_AVAILDATE", LOCATION_ID, PRODUCT_ID, CUSTOMER_GROUP, "VCCLASS", CHAR_NUM,CHARVAL_NUM;