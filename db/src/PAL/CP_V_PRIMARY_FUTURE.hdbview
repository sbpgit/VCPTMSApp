VIEW CP_V_PRIMARY_FUTURE AS
SELECT CAL_DATE, LOCATION_ID, PRODUCT_ID, CONCAT(OBJ_DEP, CONCAT('_', OBJ_COUNTER)) AS PRIMARY_ID, 
SUM(PREDICTED) AS PRIMARY_QTY
FROM CP_TS_PREDICTIONS 
WHERE MODEL_VERSION = 'Active'AND "VERSION" = '__BASELINE' AND "SCENARIO" = '_PLAN'
GROUP BY CAL_DATE, LOCATION_ID, PRODUCT_ID,OBJ_DEP,OBJ_COUNTER
ORDER BY LOCATION_ID, PRODUCT_ID, CAL_DATE, PRIMARY_ID ASC;
