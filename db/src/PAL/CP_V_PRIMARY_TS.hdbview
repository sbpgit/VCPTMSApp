VIEW CP_V_PRIMARY_TS AS
SELECT DISTINCT PP.CAL_DATE, PP.LOCATION_ID, PP.PRODUCT_ID, PP.PRIMARY_ID, PP.PRIMARY_QTY, PP.TYPE
FROM CP_V_PRIMARY_HISTORY AS PP
FULL OUTER JOIN CP_V_PRIMARY_FUTURE AS PF ON
PP.LOCATION_ID = PF.LOCATION_ID 
AND PP.PRODUCT_ID = PF.PRODUCT_ID
AND PP.PRIMARY_ID = PF.PRIMARY_ID;