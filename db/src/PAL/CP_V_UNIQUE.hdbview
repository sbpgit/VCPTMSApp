VIEW CP_V_UNIQUE AS 
SELECT DISTINCT CAL_DATE, LOCATION_ID, PRODUCT_ID, PRIMARY_ID, UNIQUE_ID, PRIMARY_QTY, UNIQUE_QTY, UNIQUE_PERCENT, "TYPE" FROM CP_V_UNIQUE_PERCENT_HISTORY
UNION SELECT CAL_DATE, LOCATION_ID, PRODUCT_ID, PRIMARY_ID, UNIQUE_ID, PRIMARY_QTY, UNIQUE_QTY, UNIQUE_PERCENT, "TYPE" FROM CP_V_UNIQUE_PERCENT_FUTURE
ORDER BY CAL_DATE, LOCATION_ID, PRODUCT_ID, PRIMARY_ID, UNIQUE_ID;