VIEW CP_V_UNIQUE_HISTORY AS
SELECT  CAL_DATE, LOCATION_ID, PRODUCT_ID, UNIQUE_ID, PRIMARY_ID, SUM (ORD_QTY) AS UNIQUE_QTY FROM CP_V_UNIQUE_BY_SALESDOC
GROUP BY CAL_DATE, LOCATION_ID, PRODUCT_ID, UNIQUE_ID, PRIMARY_ID
ORDER BY CAL_DATE, LOCATION_ID, PRODUCT_ID, UNIQUE_ID, PRIMARY_ID;
