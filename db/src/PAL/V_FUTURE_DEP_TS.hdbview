VIEW "V_FUTURE_DEP_TS"AS 
SELECT CONCAT( YEAR (TO_DATE (DEP.CAL_DATE, 'YYYY-MM-DD')), lpad(WEEK (TO_DATE(DEP.CAL_DATE, 'YYYY-MM-DD')),'2','00') ) AS "PeriodOfYear",
	   DEP."LOCATION_ID" AS "Location", DEP."PRODUCT_ID" AS "Product",   DEP."OBJ_TYPE" AS "Type", 
       DEP."OBJ_DEP" AS "OBJ_DEP", DEP."OBJ_COUNTER" AS "OBJ_COUNTER",
       CONCAT(DEP."OBJ_DEP", CONCAT('_',DEP."OBJ_COUNTER"))  AS "GroupID",
	   DEP."PREDICTED" AS "Predicted", DEP."PREDICTED_TIME" AS "PredictedTime", DEP."PREDICTED_STATUS" AS "PredictedStatus" 
	   FROM "CP_TS_OBJDEPHDR_F" DEP