VIEW "V_PREDICTION_TS" AS
SELECT CONCAT( YEAR (TO_DATE (CHARHDR.CAL_DATE, 'YYYY-MM-DD')), lpad(WEEK (TO_DATE(CHARHDR.CAL_DATE, 'YYYY-MM-DD')),'2','00') ) AS "PeriodOfYear",
	   CHARHDR."LOCATION_ID" AS "Location", CHARHDR."PRODUCT_ID" AS "Product",  CHARHDR."OBJ_TYPE" AS "Type",
       CHARHDR."OBJ_DEP" AS "OBJ_DEP", CHARHDR."OBJ_COUNTER" AS "OBJ_COUNTER", 
       CONCAT(CHARHDR."OBJ_DEP", CONCAT('_',CHARHDR."OBJ_COUNTER"))  AS "GroupID",
	   CHARHDR."ROW_ID" AS "Row",  CHARHDR."VERSION", CHARHDR."SCENARIO", CONCAT('att', CHARHDR."ROW_ID") AS "Attribute", 
	   CHARHDR."SUCCESS" AS "CharCount", CHARHDR."SUCCESS_RATE" AS "CharCountPercent"
FROM "CP_TS_OBJDEP_CHARHDR_F_TEMP" CHARHDR