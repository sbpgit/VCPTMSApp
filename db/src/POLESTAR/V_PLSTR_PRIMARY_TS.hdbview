VIEW V_PLSTR_PRIMARY_TS AS
SELECT DISTINCT WEEKDATE, PRODUCT_ID, LOCATION_ID, 
CONCAT(PARTIAL_ID_CHARS,CONCAT(',', "PRIMARY_ID_CHARS")) AS PRIMARY_CHARS_STR,  
CONCAT(PARTIAL_ID_CHARVALS,CONCAT(',', "PRIMARY_ID_CHARVALS")) AS PRIMARY_CHARVALS_STR,  
SUM(ORDER_QTY) AS ORDER_QTY
FROM V_PLSTR_ALL_IDS_BY_SALESDOC
-- WHERE PRODUCT_ID = '534EDPI'
GROUP BY WEEKDATE, PRODUCT_ID, LOCATION_ID, PARTIAL_ID_CHARS, PRIMARY_ID_CHARS, 
PARTIAL_ID_CHARVALS, PRIMARY_ID_CHARVALS
ORDER BY WEEKDATE, PRODUCT_ID, LOCATION_ID, PRIMARY_CHARVALS_STR ASC;