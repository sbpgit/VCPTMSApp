VIEW V_PLSTR_PROD_PARTIAL_CHARVALS AS 
SELECT DISTINCT PRTS.PRODUCT_ID, PRTS.LOCATION_ID, PRIMARY_ID AS PRIMARY_ID, V_CHARVAL.CLASS_NUM, CPCH.CHARVAL_NUM
FROM PLSTR_PRIMARY_TIMESERIES AS PRTS
INNER JOIN CP_PARTIALPROD_CHAR AS CPCH ON
-- PRTS.PRIMARY_ID LIKE CONCAT('%',CONCAT(CPCH.PRODUCT_ID, '%'))
PRTS.PRODUCT_ID = CPCH.PRODUCT_ID
INNER JOIN V_CHARVAL ON
CPCH.CHARVAL_NUM = V_CHARVAL.CHARVAL_NUM
ORDER BY PRODUCT_ID, LOCATION_ID, PRIMARY_ID, CHARVAL_NUM;