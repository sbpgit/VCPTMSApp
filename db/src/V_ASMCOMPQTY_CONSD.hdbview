VIEW "V_ASMCOMPQTY_CONSD"
AS
SELECT sum(COMP_QTY) AS COMP_QTY,
		CAL_DATE,
		LOCATION_ID,
		PRODUCT_ID,
		VERSION,
		SCENARIO,
		COMPONENT,
		MODEL_VERSION
	-- "COMPONENT"
	from "V_ASMCOMP_REQ"
	group by COMPONENT,
		CAL_DATE,
		LOCATION_ID,
		PRODUCT_ID,
		VERSION,
		SCENARIO,
		COMPONENT,
		MODEL_VERSION
	ORDER BY CAL_DATE DESC;