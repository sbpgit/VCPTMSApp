VIEW V_CIR_QTY_VARDESC AS
SELECT V_CIRQTY_UNIQUECHAR."LOCATION_ID",
  V_CIRQTY_UNIQUECHAR."PRODUCT_ID",
  V_CIRQTY_UNIQUECHAR."WEEK_DATE",
  V_CIRQTY_UNIQUECHAR."MODEL_VERSION",
  V_CIRQTY_UNIQUECHAR."VERSION",
  V_CIRQTY_UNIQUECHAR."SCENARIO",
  V_CIRQTY_UNIQUECHAR."CHAR_NUM",
  V_CIRQTY_UNIQUECHAR."CHARVAL_NUM",
  V_CHARVAL."CHAR_DESC",
  V_CHARVAL."CHARVAL_DESC",
  CP_VARCHAR_PS."SEQUENCE",
  V_CIRQTY_UNIQUECHAR."OPT_QTY",
  V_CIRQTY_UNIQUECHAR."CIR_QTY",
  V_CIRQTY_UNIQUECHAR."DIFF_QTY"
FROM V_CIRQTY_UNIQUECHAR
  INNER JOIN V_CHARVAL
 ON V_CHARVAL.CHAR_NUM = V_CIRQTY_UNIQUECHAR.CHAR_NUM
  AND V_CHARVAL.CHARVAL_NUM = V_CIRQTY_UNIQUECHAR.CHARVAL_NUM
  INNER JOIN CP_VARCHAR_PS
 ON CP_VARCHAR_PS.PRODUCT_ID = V_CIRQTY_UNIQUECHAR.REF_PRODID
  AND CP_VARCHAR_PS.LOCATION_ID = V_CIRQTY_UNIQUECHAR.LOCATION_ID
  AND CP_VARCHAR_PS.CHAR_NUM = V_CIRQTY_UNIQUECHAR.CHAR_NUM
ORDER BY V_CIRQTY_UNIQUECHAR."LOCATION_ID" ASC,
V_CIRQTY_UNIQUECHAR."PRODUCT_ID" ASC,
V_CIRQTY_UNIQUECHAR."WEEK_DATE" ASC,
V_CIRQTY_UNIQUECHAR."MODEL_VERSION" ASC,
V_CIRQTY_UNIQUECHAR."VERSION" ASC,
V_CIRQTY_UNIQUECHAR."SCENARIO" ASC,
V_CIRQTY_UNIQUECHAR."CHAR_NUM" ASC,
V_CIRQTY_UNIQUECHAR."CHARVAL_NUM" ASC;