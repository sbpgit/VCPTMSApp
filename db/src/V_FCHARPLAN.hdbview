VIEW "V_FCHARPLAN" AS
SELECT A."WEEK_DATE",
  A."LOCATION_ID",
  A."PRODUCT_ID",
  A."CLASS_NUM",
  B."CLASS_NAME",
  A."CHAR_NUM",
  B."CHAR_NAME",
  A."CHARVAL_NUM",
  B."CHAR_VALUE",
  A."VERSION",
  A."SCENARIO",
  A."OPT_PERCENT",
  A."OPT_QTY"
FROM "CP_IBP_FCHARPLAN" AS A
  INNER JOIN "V_CLASSCHARVAL" AS B
 ON A.CLASS_NUM = B.CLASS_NUM
  AND A.CHAR_NUM = B.CHAR_NUM