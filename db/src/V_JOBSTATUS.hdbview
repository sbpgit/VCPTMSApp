VIEW "V_JOBSTATUS"
AS
SELECT JOB_ID, JOB_NAME, "ACTION", JOB_DES, SCH_STARTTIME, 
SCH_END_TIME, SCH_TIME, SCH_NEXTRUN, JS_SCHEDULES.RUN_ID,
RUN_STATUS, RUN_STATE, STATUS_MESSAGE, SCHEDULED_TIMESTAMP,
COMPLETED_TIMESTAMP, RUNTEXT
FROM JS_JOBS 
INNER JOIN JS_SCHEDULES ON
JS_JOBS.SCHEDULE_ID = JS_SCHEDULES.SCHEDULE_ID
INNER JOIN JS_LOGS ON
JS_SCHEDULES.RUN_ID = JS_LOGS.RUN_ID;