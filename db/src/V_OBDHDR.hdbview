VIEW "V_OBDHDR"
AS SELECT B.LOCATION_ID,
        B.PRODUCT_ID,
        A.OBJ_DEP,
        A.CHAR_NAME,
        A.CHAR_VALUE,
        A.OD_CONDITION,
        A.OBJ_COUNTER,
        A.CHAR_COUNTER,
        A.ROW_ID
 FROM CP_OBJDEP_HEADER  AS A
 JOIN CP_BOM_OBJDEPENDENCY AS B
   ON A.OBJ_DEP = B.OBJ_DEP;