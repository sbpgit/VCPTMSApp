VIEW "V_ODPROFILES"
AS SELECT distinct A.LOCATION_ID,
        A.PRODUCT_ID,
        A.COMPONENT,
        A.OBJ_DEP,
        A.OBJ_COUNTER,
        A.OBJDEP_DESC,
        B.PROFILE
--  FROM CP_BOM_OBJDEPENDENCY  AS A
FROM V_OBDHDR AS A
 LEFT OUTER JOIN CP_PAL_PROFILEOD AS B ON ( A.LOCATION_ID = B.LOCATION_ID
                        AND A.PRODUCT_ID = B.PRODUCT_ID
                        AND A.COMPONENT = B.COMPONENT
                        AND A.OBJ_DEP = B.OBJ_DEP );