VIEW "V_ODRESTRICT"
AS SELECT 
A.OBJ_DEP, 
A.OBJ_COUNTER,
A.CLASS_NUM,
B.CLASS_NAME,
A.CHAR_NUM,
B.CHAR_NAME,
A.CHARVAL_NUM,
B.CHAR_VALUE,
A.OD_CONDITION,
A.CHAR_COUNTER,
A.ROW_ID
FROM CP_ODRESTRICT AS A
 LEFT OUTER JOIN V_CLASSCHARVAL AS B ON ( A.CLASS_NUM = B.CLASS_NUM
                                AND  A.CHAR_NUM  = B.CHAR_NUM
                                AND  A.CHARVAL_NUM = B.CHARVAL_NUM);