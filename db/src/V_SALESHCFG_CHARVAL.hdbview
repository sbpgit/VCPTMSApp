VIEW "V_SALESHCFG_CHARVAL"
AS SELECT 
    A.SALES_DOC,
	A.SALESDOC_ITEM,
	B.CHAR_NAME,
	B.CHAR_VALUE,
	A.PRODUCT_ID,
	-- A.PROD_AVAILDATE,
	A.CHANGED_DATE,
	A.CHANGED_BY,
	A.CREATED_DATE,
	A.CREATED_BY,
	A.CHANGED_TIME,
	A.CREATED_TIME
FROM CP_SALESH_CONFIG AS A
INNER JOIN V_CHARVAL AS B
ON A.CHAR_NUM = B.CHAR_NUM
AND A.CHARVAL_NUM = B.CHARVAL_NUM;
