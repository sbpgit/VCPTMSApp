VIEW "V_TSODCHAR_H" AS
SELECT a."CAL_DATE",
  a."OBJ_DEP",
  a."OBJ_COUNTER",
  a."SUCCESS",
  b."ROW_ID",
  b."SUCCESS" AS "CHAR_SUCCESS"
FROM "CP_TS_OBJDEPHDR" AS a
  INNER JOIN "CP_TS_OBJDEP_CHARHDR" AS b
 ON a."CAL_DATE" = b."CAL_DATE"
  AND a."OBJ_DEP" = b."OBJ_DEP"
  AND a."OBJ_COUNTER" = b."OBJ_COUNTER"
ORDER BY a."OBJ_DEP",
  a."OBJ_COUNTER" DESC