COLUMN TABLE "PLSTR_PRODUCT_CLASS"(
	"PRODUCT_ID" NVARCHAR(40) NOT NULL,
	"CLASS_NUM" NVARCHAR(18) NOT NULL,
	"CHANGED_DATE" DATE,
	"CHANGED_BY" NVARCHAR(12),
	"CREATED_DATE" DATE,
	"CREATED_BY" NVARCHAR(12),
	"CHANGED_TIME" TIME,
	"CREATED_TIME" TIME,
	PRIMARY KEY INVERTED INDIVIDUAL(
		"PRODUCT_ID",
		"CLASS_NUM"
	)
);
