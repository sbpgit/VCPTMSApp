VIEW V_PLSTR_PROD_CHAR_CHARVALS AS
SELECT 	"PRODUCT_ID", "LOCATION_ID",  "PRIMARY_ID", PIDCH."CLASS_NUM", CHAR_NUM, PIDCH."CHARVAL_NUM"
FROM "PLSTR_PRODUCT_ID_CHARVALS" AS PIDCH
INNER JOIN V_CHARVAL AS VCH ON
PIDCH.CLASS_NUM = VCH.CLASS_NUM AND
PIDCH.CHARVAL_NUM = VCH.CHARVAL_NUM
ORDER BY PRODUCT_ID, PRIMARY_ID,CHAR_NUM,CHARVAL_NUM;