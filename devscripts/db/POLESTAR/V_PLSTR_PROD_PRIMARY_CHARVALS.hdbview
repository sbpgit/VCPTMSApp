VIEW V_PLSTR_PROD_PRIMARY_CHARVALS AS
SELECT DISTINCT PIDS.PRODUCT_ID, PIDS.LOCATION_ID, PRIMARY_ID, V_CHARVAL.CLASS_NUM, PIDS.CHAR_VALUE AS CHARVAL_NUM
FROM PLSTR_PRIMARY_TIMESERIES AS PIDS
INNER JOIN V_CHARVAL ON
PIDS.CHAR_VALUE = V_CHARVAL.CHARVAL_NUM
ORDER BY PRODUCT_ID, LOCATION_ID, PRIMARY_ID, CHARVAL_NUM;