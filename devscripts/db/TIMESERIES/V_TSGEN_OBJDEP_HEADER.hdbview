VIEW "V_TSGEN_OBJDEP_HEADER" ( "OBJ_DEP", "OBJ_COUNTER", "CLASS_NUM", "CHAR_NUM", "CHARVALS", "OD_CONDITION", "ROW_ID" ) AS 
(SELECT DISTINCT "OBJ_DEP", "OBJ_COUNTER", "CLASS_NUM", "CHAR_NUM",
CONCAT('(', CONCAT(STRING_AGG(CONCAT('''', CONCAT("CHARVAL_NUM",'''')),','),')'))  AS "CHARVALS",
"OD_CONDITION", "ROW_ID"
FROM "CP_OBJDEP_HEADER" 
GROUP BY "OBJ_DEP", "OBJ_COUNTER", "CLASS_NUM","ROW_ID", "CHAR_NUM", "OD_CONDITION" ORDER BY "OBJ_DEP", "OBJ_COUNTER", "CLASS_NUM",  "ROW_ID", "CHAR_NUM", "OD_CONDITION" ASC) WITH READ ONLY