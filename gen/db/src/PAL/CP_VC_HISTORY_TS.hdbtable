COLUMN TABLE "CP_VC_HISTORY_TS"(
  "PERIOD_NUM" INTEGER NOT NULL,
  "LOCATION_ID" NVARCHAR(5) NOT NULL,
  "PRODUCT_ID" NVARCHAR(40) NOT NULL,
  "TYPE" NVARCHAR(2) NOT NULL,
  "GROUP_ID" NVARCHAR(100) NOT NULL,
  "ROW" INTEGER NOT NULL,
  "ATTRIBUTE" NVARCHAR(10) NOT NULL,
  "CHAR_COUNT" DOUBLE,
  "CHAR_COUNT_RATE" DOUBLE,
  "GROUP_COUNT" DOUBLE,
  "GROUP_COUNT_RATE" DOUBLE,
  PRIMARY KEY (
    "PERIOD_NUM",
    "LOCATION_ID",
    "PRODUCT_ID",
    "TYPE",
    "GROUP_ID",
    "ROW",
    "ATTRIBUTE"
  )
);