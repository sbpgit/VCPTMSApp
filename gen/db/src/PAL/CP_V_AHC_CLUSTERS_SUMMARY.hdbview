VIEW CP_V_AHC_CLUSTERS_SUMMARY AS
SELECT AHCR.LOCATION_ID, AHCR.PRODUCT_ID, PROFILE, STAGE, LEFT_ID_CLUSTER, RIGHT_ID_CLUSTER,UNIQUE_ID,
LEFT_ID AS UNIQUE_ID_LEFT, RIGHT_ID AS UNIQUE_ID_RIGHT,  DISTANCE, 
C1, C2, C3, C4, C5, C6, C7, C8, C9, C10, C11, C12, C13, C14, C15, C16, C17, C18, C19, C20
FROM CP_V_AHC_CLUSTER_RESULTS  AHCR
INNER JOIN  CP_CLUSTER_DATA AS CD ON
AHCR.LOCATION_ID = CD.LOCATION_ID
AND AHCR.PRODUCT_ID = CD.PRODUCT_ID
AND (AHCR.LEFT_ID = CD.UNIQUE_ID  OR AHCR.RIGHT_ID = CD.UNIQUE_ID)
ORDER BY LEFT_ID_CLUSTER, CAST(UNIQUE_ID_LEFT AS INTEGER),DISTANCE;