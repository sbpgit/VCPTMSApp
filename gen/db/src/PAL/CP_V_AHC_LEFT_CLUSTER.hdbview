VIEW CP_V_AHC_LEFT_CLUSTER AS
SELECT
    AHCR."LOCATION_ID" AS LOCATION_ID,
    AHCR."PRODUCT_ID" AS PRODUCT_ID,
    AHCR."MODEL_PROFILE" AS PROFILE,
	AHCR."CLUSTER_ID" AS LEFT_ID_CLUSTER,
	LEFT_ID,
	RIGHT_ID,
	DISTANCE 
FROM "CP_AHC_RESULTS" AS AHCR
INNER JOIN "CP_AHC_COMBINE_PROCESS" AS AHCCP ON
AHCR.UNIQUE_ID = LEFT_ID AND
AHCR.LOCATION_ID = AHCCP.LOCATION_ID AND 
AHCR.PRODUCT_ID = AHCCP.PRODUCT_ID AND 
AHCR.MODEL_PROFILE = AHCCP.MODEL_PROFILE 
ORDER BY AHCR."LOCATION_ID", AHCR."PRODUCT_ID", PROFILE, CLUSTER_ID;