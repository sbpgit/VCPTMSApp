VIEW CP_V_UNIQUE_HISTORY_AVG_PERCENT AS
SELECT TUQ.LOCATION_ID, TUQ.PRODUCT_ID, TUQ.PRIMARY_ID, TUQ.UNIQUE_ID, TPQ.PRIMARY_QTY, UNIQUE_QTY,
100*(UNIQUE_QTY/PRIMARY_QTY) AS UNIQUE_PERCENT
FROM CP_V_UNIQUE_HISTORY_QTY  AS TUQ
INNER JOIN CP_V_PRIMARY_HISTORY_QTY AS TPQ ON
TUQ.LOCATION_ID =  TPQ.LOCATION_ID
AND TUQ.PRODUCT_ID =  TPQ.PRODUCT_ID
AND TUQ.PRIMARY_ID =  TPQ.PRIMARY_ID
ORDER BY TUQ.LOCATION_ID, TUQ.PRODUCT_ID, TUQ.PRIMARY_ID,TUQ.UNIQUE_ID;