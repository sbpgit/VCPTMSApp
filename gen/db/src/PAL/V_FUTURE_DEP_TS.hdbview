VIEW "V_FUTURE_DEP_TS"AS 
SELECT DISTINCT DEP."CAL_DATE", CONCAT( YEAR (TO_DATE (DEP.CAL_DATE, 'YYYY-MM-DD')), lpad(WEEK (TO_DATE(DEP.CAL_DATE, 'YYYY-MM-DD')),'2','00') ) AS "PERIOD_NUM",
	   DEP."LOCATION_ID" AS "Location", DEP."PRODUCT_ID" AS "Product",   DEP."OBJ_TYPE" AS "Type", 
       DEP."OBJ_DEP" AS "OBJ_DEP", DEP."OBJ_COUNTER" AS "OBJ_COUNTER",
       DEP."ROW_ID",
       DEP."SUCCESS" AS "CharCount",
       DEP."SUCCESS_RATE" AS "CharCountPercent",
       CASE WHEN DEP."SUCCESS_RATE" > 0 THEN DEP."SUCCESS"/DEP."SUCCESS_RATE" ELSE '0' END AS "OrderQuantity",
       DEP."VERSION", DEP."SCENARIO",
       CONCAT(CONCAT(DEP."OBJ_DEP", CONCAT('_',DEP."OBJ_COUNTER")),CONCAT(CONCAT('#',DEP."LOCATION_ID"),CONCAT('#',DEP."PRODUCT_ID")))  AS "GroupID"
	   FROM "CP_TS_OBJDEP_CHARHDR_F" DEP