COLUMN TABLE "PLSTR_CHARACTERISTICS"(
	"CLASS_NUM" NVARCHAR(18) NOT NULL,
	"CHAR_NUM" NVARCHAR(10) NOT NULL,
	"CHAR_NAME" NVARCHAR(30),
	"CHAR_DESC" NVARCHAR(150),
	"CHAR_GROUP" NVARCHAR(10),
	"CHAR_TYPE" NVARCHAR(4),
	"ENTRY_REQ" NVARCHAR(1),
	"CHAR_CATGRY" NVARCHAR(40),
	PRIMARY KEY INVERTED INDIVIDUAL(
		"CLASS_NUM",
		"CHAR_NUM"
	)
);
