COLUMN TABLE "PLSTR_PRIMARY_TIMESERIES"(
    "WEEKDATE" DATE NOT NULL,
    "PRODUCT_ID" NVARCHAR(40),
    "LOCATION_ID" NVARCHAR(4),
    "PRIMARY_ID" NVARCHAR(50) NOT NULL,
    "CHAR_VALUE" NVARCHAR(70),
    "ORD_QTY" DECIMAL(13, 3),
     PRIMARY KEY INVERTED INDIVIDUAL(
		"WEEKDATE",
		"PRODUCT_ID",
        "LOCATION_ID",
        "PRIMARY_ID",
        "CHAR_VALUE"
	)
);