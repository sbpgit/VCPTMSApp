VIEW V_PLSTR_CIR_AT_LOCPROD AS
SELECT PRTS.CAL_DATE, PRTS.LOCATION_ID, PRTS.PRODUCT_ID, PRTS.MODEL_TYPE, PRTS.MODEL_VERSION,
PRTS.MODEL_PROFILE, PRTS.VERSION, CHAR_NUM, CHARVAL_NUM, "CIR", 100.0*("CIR")/ "DEMANDATLOCPROD" AS "CIR_PERCENT" 
FROM V_PLSTR_PARTIAL_CIR_TS PRTS
INNER JOIN V_PLSTR_PREDCITED_DMD_AT_LOCPROD_TS AS DMDTS ON
 PRTS.CAL_DATE = DMDTS.CAL_DATE AND 
 PRTS.LOCATION_ID = DMDTS.LOCATION_ID AND 
 PRTS.PRODUCT_ID  = DMDTS.PRODUCT_ID AND 
 PRTS.MODEL_TYPE  = DMDTS.MODEL_TYPE AND
 PRTS.MODEL_VERSION  = DMDTS.MODEL_VERSION AND
 PRTS.SCENARIO  = DMDTS.SCENARIO
ORDER BY PRTS.CAL_DATE, PRTS.LOCATION_ID, PRTS.PRODUCT_ID, PRTS.MODEL_TYPE, PRTS.MODEL_VERSION,
PRTS.MODEL_PROFILE, PRTS.VERSION, CHAR_NUM, CHARVAL_NUM;