VIEW V_PLSTR_PRIMARY_CIR_TS AS 
SELECT CAL_DATE, PD.LOCATION_ID, PD.PRODUCT_ID, PRIMARY_ID, MODEL_TYPE, PD.MODEL_VERSION, MODEL_PROFILE, VERSION, SCENARIO, 
CHAR_NUM, CHARVAL_NUM, PREDICTED AS CIR
FROM CP_TS_PREDICTIONS AS PD
INNER JOIN V_PLSTR_PROD_CHAR_CHARVALS AS PCH ON
PD.LOCATION_ID = PCH.LOCATION_ID AND
PD.PRODUCT_ID = PCH.PRODUCT_ID AND
PRIMARY_ID = CONCAT(OBJ_DEP, CONCAT('_',OBJ_COUNTER));