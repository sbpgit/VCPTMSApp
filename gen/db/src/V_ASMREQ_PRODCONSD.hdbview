VIEW "V_ASMREQ_PRODCONSD" AS
SELECT "LOCATION_ID",
  "REF_PRODID" AS PRODUCT_ID,
  "ITEM_NUM",
  "COMPONENT",
  "WEEK_DATE",
  "MODEL_VERSION",
  "VERSION",
  "SCENARIO",
  SUM("COMPCIR_QTY") AS COMPCIR_QTY
FROM "CP_ASSEMBLY_REQ"
GROUP BY "LOCATION_ID",
  "REF_PRODID",
  "ITEM_NUM",
  "COMPONENT",
  "WEEK_DATE",
  "MODEL_VERSION",
  "VERSION",
  "SCENARIO";