VIEW "V_PLANNEDCONFIG" AS
SELECT DISTINCT
    A.GROUP_ID,
    A.SEQUENCE,
    A.PARAMETER_ID,
    A.DESCRIPTION,
    A.MIN_VALUE,
    A.MAX_VALUE,
    A.VALUE_HELP,
    A.VALUE_HELP_TAB,
    B.GROUP_DESCRIPTION,
    B.UNIT,
    C.LOCATION_ID,
    C.VALUE  
FROM CP_PLANNED_PARAMETERS AS A
INNER JOIN CP_PLANNED_GROUPS AS B
 ON A.GROUP_ID = B.GROUP_ID
LEFT OUTER JOIN CP_PARAMETER_VALUES AS C
 ON A.PARAMETER_ID = C.PARAMETER_ID
 ORDER BY A.GROUP_ID ASC, 
          A.SEQUENCE ASC;