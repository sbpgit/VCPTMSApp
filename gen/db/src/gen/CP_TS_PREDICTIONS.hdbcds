entity CP_TS_PREDICTIONS {
  key CAL_DATE : LocalDate;
  key LOCATION_ID : String(4);
  key PRODUCT_ID : String(40);
  key OBJ_TYPE : String(2);
  key OBJ_DEP : String(30);
  key OBJ_COUNTER : Integer;
  MODEL_TYPE : String(10);
  key MODEL_VERSION : String(20);
  MODEL_PROFILE : String(50);
  key VERSION : String(10);
  key SCENARIO : String(32);
  PREDICTED : Double;
  PREDICTED_TIME : UTCTimestamp;
  PREDICTED_STATUS : String(8);
};
