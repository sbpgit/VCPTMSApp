service MetadataService @(requires: ['authenticated-user', 'system-user']) {

    aspect tenantRelated {
        key tenantId: UUID;
    }
    @readonly
    entity edmx: tenantRelated {
        language: String(4);
        name: String(100);
        odataVersion: String(2);
    }

    @readonly
    entity csn: tenantRelated { }
    // type CSN: String;
    // function csn(tenantId: UUID) returns CSN;

    @readonly
    entity node_csn  {
       key tenantId: UUID;
    }

    @readonly
    entity languages {
       key tenantId: UUID;
    }
    // function languages(tenantId: UUID) returns array of String;

    @readonly
    entity services: tenantRelated { }
    // function services(tenantId: UUID) returns array of String;

}
