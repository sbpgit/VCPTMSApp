  "cds": {
    "requires": {
      "auth": {
        "kind": "basic-auth",
        "users": {
          "SBPTECHTEAM": {
            "password": "Sbpcorp@22"
          },
          "*": false
        }
      },
      "build": {
        "target": "."
      },
      "db": {
        "kind": "hana",
        "pool": {
          "acquireTimeoutMillis": 120000,
          "evictionRunIntervalMillis": 120000,
          "min": 50,
          "max": 500,
          "numTestsPerEvictionRun": 60,
          "softIdleTimeoutMillis": 30000,
          "idleTimeoutMillis": 30000,
          "testOnBorrow": true
        }
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "IBPDemandsrv": {
        "kind": "odata-v2",
        "model": "srv/external/IBPDemandsrv",
        "credentials": {
          "destination": "IBPDemandsrv"        
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
