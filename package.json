    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "features": {
            "fetch_csrf": true
        },
        "requires": {
            "build": {
                "tasks": [
                    {
                        "for": "hana",
                        "dest": "../db"
                    },
                    {
                        "for": "node-cf"
                    }
                ]
            },
            "db": {
                "kind": "hana",
                "pool": {
                    "acquireTimeoutMillis": 120000,
                    "evictionRunIntervalMillis": 120000,
                    "min": 50,
                    "max": 500,
                    "numTestsPerEvictionRun": 60,
                    "softIdleTimeoutMillis": 30000,
                    "idleTimeoutMillis": 30000,
                    "testOnBorrow": true
                }
            },
            "uaa": {
                "kind": "xsuaa"
            },
            "IBPDemandsrv": {
                "kind": "odata-v2",
                "model": "srv/external/IBPDemandsrv",
                "[development]": {
                    "credentials": {
                        "url": "https://my400323-api.scmibp.ondemand.com/sap/opu/odata/ibp/PLANNING_DATA_API_SRV",
                        "authentication": "BasicAuthentication",
                        "username": "BTPUSER",
                        "password": "Sbp@btpproductdevelopment01"
                    }
                },
                "[production]": {
                    "credentials": {
                        "destination": "IBPExternal",
                        "path": "/sap/opu/odata/ibp/PLANNING_DATA_API_SRV"
                    }
                }
            },
            "IBPMasterDataAPI": {
                "kind": "odata-v2",
                "model": "srv/external/IBPMasterDataAPI",
                "[development]": {
                    "credentials": {
                        "url": "https://my400323-api.scmibp.ondemand.com/sap/opu/odata/IBP/MASTER_DATA_API_SRV",
                        "authentication": "BasicAuthentication",
                        "username": "BTPUSER",
                        "password": "Sbp@btpproductdevelopment01"
                    }
                },
                "[production]": {
                    "credentials": {
                        "destination": "IBPExternal",
                        "path": "/sap/opu/odata/IBP/MASTER_DATA_API_SRV"
                    }
                }
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
