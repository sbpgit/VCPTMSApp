    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "requires": {
            "auth": {
                "kind": "basic-auth",
                "users": {
                    "SBPTECHTEAM": {
                        "password": "Sbpcorp@22"
                    },
                    "*": false
                }
            },
            "db": {
                "kind": "hana",
                "pool": {
                    "acquireTimeoutMillis": 120000,
                    "evictionRunIntervalMillis": 120000,
                    "min": 50,
                    "max": 500,
                    "numTestsPerEvictionRun": 60,
                    "softIdleTimeoutMillis": 30000,
                    "idleTimeoutMillis": 30000,
                    "testOnBorrow": true
                }
            },
            "uaa": {
                "kind": "xsuaa"
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    },
    "sapux": [
        "app/cfgprd_pvsroot",
        "app/project1",
        "app/cp_ibpfdem",
        "app/cp_ibpfplan",
        "app/cp_ibpfres",
        "app/cp_execprediction",
        "app/cp_exeregressionmdls",
        "app/cp_modelprofiles",
        "app/cp_assignppf",
        "app/cp_odprofiles",
        "app/cp_assignprofiles",
        "app/cp_locprod",
        "app/cp_saleshcfg",
        "app/cpodrestriction",
        "app/cpibpresultplan",
        "app/cpodrestrict",
        "app/cpprodaccnode",
        "app/cpcompsnode",
        "app/cplocation"
    ],
    "devDependencies": {
        "@sap/ux-specification": "^1.96.3"
    }
}